def getData(html):
	info = []
	r = requests.get(url, timeout=30)
	r.raise_for_status
	r.encoding = r.apparent_encoding
	html = r.text
	try:
		data4 = re.search(r'<h1>(.*?)</h1>',html, re.S)
		s1 = data4.group(1)
	except:
		s1 = '001'

	# 获取“课程类型”、“持续时间”、“学费”
	try:
		data1 = re.search(
			r'<dt>Course Type</dt><dd>(.*?)</dd><dt>Duration</dt><dd>(.*?)</dd><dt>Fees and funding</dt><dd><strong>(.*?)<br /><a title',
			html, re.S, )
		t1 = data1.group(1)
		t2 = data1.group(2)
		t3 = data1.group(3)
	except AttributeError as e:
		try:
			data1 = re.search(
				r'<dt>Course Type</dt><dd>(.*?)</dd><dt>Duration</dt><dd>(.*?)</dd><dt>Fees and funding</dt><dd><p>(.*?)More detail</a>',
				html, re.S, )
			t1 = data1.group(1)
			t2 = data1.group(2)
			t3 = data1.group(3)
		except AttributeError as e:
			try:
				data1 = re.search(
					r'<dt>Course Type</dt><dd>(.*?)</dd><dt>Duration</dt><dd>(.*?)</dd><dt>Fees and funding</dt><dd><p><b>(.*?)<br /><a href',
					html, re.S, )
				t1 = data1.group(1)
				t2 = data1.group(2)
				t3 = data1.group(3)
			except AttributeError as e:
				try:
					data1 = re.search(
						r'<dt>Course Type</dt><dd>(.*?)</dd><dt>Duration</dt><dd>(.*?)</dd><dt>Fees and funding</dt><dd><b>(.*?)<br /><a href',
						html, re.S, )
					t1 = data1.group(1)
					t2 = data1.group(2)
					t3 = data1.group(3)
				except AttributeError as e:
					try:
						data1 = re.search(
							r'<dt>Course Type</dt><dd>(.*?)</dd><dt>Duration</dt><dd>(.*?)</dd><dt>Fees and funding</dt><dd>(.*?)<br /><a title',
							html, re.S, )
						t1 = data1.group(1)
						t2 = data1.group(2)
						t3 = data1.group(3)
					except AttributeError as e:
						try:
							data1 = re.search(
								r'<dt>Course Type</dt><dd>(.*?)</dd><dt>Duration</dt><dd>(.*?)</dd><dt>Fees and funding</dt><dd><p>(.*?)</a></p></dd>',
								html, re.S, )
							t1 = data1.group(1)
							t2 = data1.group(2)
							t3 = data1.group(3)
						except:
							t1 = '001'
							t2 = '001'
							t3 = '001'

	# 获取“开始日期”、“学习选择”
	try:
		data2 = re.search(
			r'<dt>Start date</dt><dd>(.*?)</dd><dt>Study options</dt><dd>(.*?)</dd>',
			html, re.S)
		u1 = data2.group(1)
		u2 = data2.group(2)
	except:
		u1 = '456'
		u2 = '456'

	# 获取“入学要求”、“如何申请”
	try:
		data3 = re.search(
			r'</header><div id="EntryRequirements_content" class="accordion__body" role="region">(.*?)</section><section id="HowToApply" class="accordion__block">.*?</header><div id="HowToApply_content" class="accordion__body" role="region">(.*?)>How to apply pages',
			html, re.S, )
		v1 = data3.group(1)
		v2 = data3.group(2)
	except AttributeError as e:
		try:
			data3 = re.search(
				r'</header><div id="EntryRequirements_content" class="accordion__body" role="region">(.*?)>Standard English',
				html, re.S, )
			v1 = data3.group(1)
			v2 = '789'
		except:
			v1 = '789'
			v2 = '789'

	info.append(s1)
	info.append(t1)
	info.append(t2)
	info.append(t3)
	info.append(u1)
	info.append(u2)
	info.append(v1)
	info.append(v2)
	info.append(url)
	return info

def write_to_csv(data):
    out = open('Birmingham.csv', 'a', newline='',encoding='utf-8')#csv文件写入链接时需要指定编码格式为utf-8
    csv_write = csv.writer(out, dialect='excel')
    csv_write.writerow(data)

if __name__ == '__main__':
	count = 0
	for url in urls[350:]:
		count = count +1
		print("正在获取第{n}个页面数据".format(n=count))
		gift = getData(url)
		print("正在写入第{n}个页面数据".format(n=count))
		write_to_csv(gift)
		time.sleep(30)

